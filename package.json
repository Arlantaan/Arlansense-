{
  "name": "sensation-by-sanu",
  "version": "1.0.0",
  "description": "Sensation by Sanu - Perfume Boutique E-commerce Platform",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "dev:xampp": "powershell -ExecutionPolicy Bypass -File setup-xampp-dev.ps1",
    "dev:backend": "powershell -ExecutionPolicy Bypass -File dev-backend-xampp.ps1",
    "test:api": "powershell -ExecutionPolicy Bypass -File test-backend-api.ps1",
    "dev:frontend": "npm run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "@react-three/drei": "^9.56.12",
    "@react-three/fiber": "^8.10.1",
    "axios": "^1.3.4",
    "framer-motion": "^10.0.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.8.1",
    "three": "^0.150.1",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/three": "^0.150.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.0"
  }
}
